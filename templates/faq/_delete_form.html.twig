<form method="post" action="{{ path('faq_delete', {'id': faq.id}) }}"
      onsubmit="return confirm('Voulez-vous vraiment supprimer?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ faq.id) }}">
    <button class="btn btn-round-danger faq-danger-btn">Supprimer</button>
</form>
